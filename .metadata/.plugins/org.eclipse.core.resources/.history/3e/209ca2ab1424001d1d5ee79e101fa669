package ch4;

public class Op1 {

	public static void main(String[] args) {
		int a = 10;
			System.out.println(a);
		int b1 = a++;
			System.out.println(b1);
				
		int b2 = ++a;
			System.out.println(a);
			System.out.println(b2);

		int b3 = a--;
			System.out.println(a);
			System.out.println(b3);
	}

}
