package This_is;

public class p75_BitReverseOperator_EX {

	public static void main(String[] args) {
		int v1 = 10;
		int v2 = ~v1;
		int v3 = ~v1 + 1;
		
		System.out.println(v1) + " (십진수: " + v1 + ")");
		System.out.println(v2) + " (십진수: " + v2 + ")");
		System.out.println(v3) + " (십진수: " + v3 + ")");
		System.out.println();
	}

}