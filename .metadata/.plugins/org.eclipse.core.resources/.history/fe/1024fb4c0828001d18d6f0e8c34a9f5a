package Quiz_aznara;

// º¹½À

public class Sand_Watch_5 {

	public static void main(String[] args) {
		for(int i = 1; i <= 5; i++) {
			for (int j = 1; j <= 5-i; j++) {
				System.out.print(" ");
			}
			for(int k = 1; j <=i*2-1; k++) {
				System.out.println("*");
			}
		System.out.println();
		}
	}
}