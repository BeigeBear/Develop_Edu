import java.util.Scanner;

public class Arr2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int i, max;
		int[] arr = new int[10];
		System.out.print("정수 10개를 입력하라.");
		for (i=0; i<arr.length; i++) {
			arr[i] = sc.nextInt();
		
		}
		
		max = arr[0];
		
		for(i = 0; i< arr.length; i++) {
			if(max < arr[i]) {
				max = arr[i];
			}
		}
		System.out.println("max = "+ max);

		
	}

}
