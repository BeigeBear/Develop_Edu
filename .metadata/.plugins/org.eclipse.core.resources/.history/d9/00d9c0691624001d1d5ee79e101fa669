package ch4;

public class Op1 {

	public static void main(String[] args) {
		int a = 7;
		
		int b1 = a++;
			System.out.println(a); // a+1
			System.out.println(b1); // 10
				
		int b2 = ++a;
			System.out.println(a); //
			System.out.println(b2);

		int b3 = a--;
			System.out.println(a);
			System.out.println(b3);
			
		int b4 = --a;
			System.out.println(a);
			System.out.println(b4);
	}

}
