package This_is;

public class p59_OperationsPromotion_EX {

	public static void main(String[] args) {
		byte byteValue1 = 10;
		byte byteValue2 = 20;
//		byte byteValue3 = byteValue1 + byteValue2;  연산이 int로 이루어지기에 컴파일 에러.
		int intValue1 = byteValue1 + byteValue2;
		System.out.println(intValue1);
		
		char charValue1 = 'A';
		char charValue2 = '1';
//		char charValue3 = charValue1 + charValue2;  연산이 int로 이뤄짐, 컴파일 에러.
		System.out.println("유니코드=" + intValue2);
		System.out.println("출력문자=" + (char)intValue2);
		
	}
}