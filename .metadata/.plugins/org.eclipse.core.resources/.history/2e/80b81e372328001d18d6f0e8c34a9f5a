package ThisIsJava;

public class p127_While_Key_Contral_ex {

	public static void main(String[] args) {
		boolean run = true;
		int speed = 0;
		int keycode = 0;
		
		while(run) {
			if(keycode!=13 && keycode! = 10) {
				System.out.println("--------------------");
				System.out.println("1. 종속 | 2. 감속 | 3. 중지");
				System.out.println("--------------------");
				System.out.print("선택 : ");
				
			}
		}

	}

}
