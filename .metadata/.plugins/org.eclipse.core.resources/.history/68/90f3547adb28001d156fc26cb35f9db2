
public class Only_Exit_Return {

	public static void main(String[] args) {
		divide1(4,2);
		divide1(6,2);
		divide1(9,2);
	}	
	
	public static void divide(int num1, num2) {
		if(num2 == 0) {
			System.out.println("0으로 나눌 수 없습니다.");
			return;
		}
		
		System.out.println("나눗셈 결과: "+ (num1+num2));
	}

}
