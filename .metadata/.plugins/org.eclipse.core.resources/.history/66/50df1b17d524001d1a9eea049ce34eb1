package This_is;

public class P100_TernaryOperator {

	public static void main(String[] args) {
		int score = 95;
		char grade = (score>90) ? 'A' : 'B';

		System.out.println(grade);
		
		
//		=====================================
		
		int score2 = 95;
		char grade2;
		if(score>90) {
			grade2 = 'A';
		} else {
			grade2 = 'B';
		}
	}

}
